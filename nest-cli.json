{
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "plugins": [
            {
                "name": "@nestjs/graphql",
                "options": {
                    "typeFileNameSuffix": [".input.ts", ".args.ts", ".response.ts", ".model.ts"],
                    "introspectComments": true
                }
            }
        ]
    }
}
